    "def get_string():\n",
    "    return str(input('Masukkan kalimat yang ingin anda balikkan \\n'))\n",
    "def reverse_name(ab):\n",
    "    cd = ab.split()\n",
    "    cd.reverse()\n",
    "    hasil_akhir = \" \".join(cd)\n",
    "    return hasil_akhir\n",
    "\n",
    "##Kodingan\n",
    "\n",
    "testing = get_string()\n",
    "print(reverse_name(testing))\n"
